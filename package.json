{
  "name": "monaco-editor-esm-i18n",
  "version": "1.0.1",
  "description": "A wrapper to generate an es5 monaco editor with i18n, specifically developed for node-red",
  "main": "index.js",
  "scripts": {
    "build": "npm run update-types && npm run generate && npm run generate-themes && npm run pack",
    "clean": "rm -r -f output && rm -r -f temp",
    "generate": "node generate-monaco-esm-i18n.js",
    "generate-themes": "node generate-themes.js",
    "update-types": "node update-types.js",
    "pack": "npx webpack",
    "demo": "npx http-server",
    "generate-extraLibs": "node generate-extraLibs.js"
  },
  "keywords": [
    "monaco"
  ],
  "author": "steve-mcl",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^14.14.45",
    "copy-webpack-plugin": "^8.1.1",
    "css-loader": "^5.2.6",
    "dts-minify": "^0.2.3",
    "git-pull-or-clone": "^2.0.1",
    "glob": "^7.1.7",
    "insane": "^2.6.2",
    "mkdirp": "^1.0.4",
    "monaco-editor": "^0.25.2",
    "monaco-themes": "^0.3.3",
    "ncp": "^2.0.0",
    "read-package-json": "^3.0.1",
    "recursive-readdir": "^2.2.2",
    "replace-in-file": "^6.2.0",
    "rimraf": "^3.0.2",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.1.3",
    "typedoc": "^0.20.36",
    "typescript": "^4.3.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.39.1",
    "webpack-cli": "^4.7.2"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "staticFiles": {
    "staticPath": [
      {
        "outDirPattern": "**/dist",
        "staticPath": "theme",
        "staticOutDir": "theme"
      },
      {
        "outDirPattern": "**/dist-legacy",
        "staticPath": "theme",
        "staticOutDir": "theme"
      }
    ]
  }
}
